<UserControl x:Class="Clipple.View.Waveform"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:view="clr-namespace:Clipple.View"
             mc:Ignorable="d"
             d:DesignHeight="300" d:DesignWidth="300" IsVisibleChanged="OnIsVisibleChanged" Unloaded="OnUnloaded">
    <Grid Width="{x:Static view:Waveform.DoubleResolutionX}" Height="{x:Static view:Waveform.DoubleResolutionY}">
        <Image x:Name="Image" RenderOptions.BitmapScalingMode="HighQuality" />
    </Grid>
</UserControl>
